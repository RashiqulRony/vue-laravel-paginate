{
  "name": "vue-laravel-paginate",
  "version": "1.0.6",
  "description": "This is vue js package, using to Laravel API",
  "main": "dist/vue-laravel-paginate.ssr.js",
  "browser": "dist/vue-laravel-paginate.esm.js",
  "module": "dist/vue-laravel-paginate.esm.js",
  "unpkg": "dist/vue-laravel-paginate.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/RashiqulRony/vue-laravel-paginate"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "Vue JS",
    "Pagination",
    "Vue Pagination",
    "Vue Laravel Paginate"
  ],
  "homepage": "https://github.com/RashiqulRony",
  "author": "Rashiqul Rony <rashiqulrony@gmail.com> (https://github.com/RashiqulRony)"
}
